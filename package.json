{
	"name": "vscode-issue-explorer",
	"displayName": "VS Code Issue Explorer",
	"description": "A VS Code extension that helps to create and explore GitHub issues within your IDE.",
	"version": "0.0.1",
	"license": "MIT",
	"publisher": "stateful",
	"icon": ".github/assets/stateful.png",
	"galleryBanner": {
		"color": "#2F2F2F",
		"theme": "dark"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/stateful/vscode-issue-explorer.git"
	},
	"engines": {
		"vscode": "^1.69.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"vscode",
		"extension",
		"best practices"
	],
	"activationEvents": [
		"onStartupFinished"
	],
  "type": "module",
	"main": "./out/index.js",
  "exports": "./out/index.js",
	"extensionKind": [
		"ui"
	],
	"contributes": {
		"configuration": [],
		"commands": [],
		"views": {}
	},
	"scripts": {
		"build": "run-s clean compile",
		"clean": "rimraf out",
		"compile": "rollup -c ./rollup.config.mjs",
		"test": "run-s test:*",
		"test:lint": "eslint src --ext ts",
		"test:e2e": "cd test && npx wdio run ./wdio.conf.ts",
		"watch": "npm run clean && npm run compile -- --watch"
	},
	"devDependencies": {
		"@rollup/plugin-node-resolve": "^13.3.0",
		"@rollup/plugin-typescript": "^8.3.3",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "18.x",
		"@types/vscode": "^1.69.0",
		"@typescript-eslint/eslint-plugin": "^5.30.6",
		"@typescript-eslint/parser": "^5.30.6",
		"@vscode/test-electron": "^2.1.5",
		"@wdio/cli": "^7.20.5",
		"@wdio/local-runner": "^7.20.5",
		"@wdio/mocha-framework": "^7.20.3",
		"@wdio/spec-reporter": "^7.20.3",
		"eslint": "^8.19.0",
		"eta": "^1.12.3",
		"npm-run-all": "^4.1.5",
		"rimraf": "^3.0.2",
		"rollup": "^2.76.0",
		"rollup-plugin-eta": "^1.0.0",
		"tslib": "^2.4.0",
		"ts-node": "^10.8.2",
		"typescript": "^4.7.4",
		"vsce": "^2.9.2",
		"wdio-vscode-service": "^3.0.3",
		"webdriverio": "^7.20.5"
	},
	"dependencies": {
		"@vscode/codicons": "^0.0.31",
		"@vscode/webview-ui-toolkit": "^1.0.0",
		"lit-element": "^3.2.1",
		"rollup-plugin-multi-input": "^1.3.1",
		"rxjs": "^7.5.6",
		"tangle": "^2.1.0"
	}
}
