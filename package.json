{
  "name": "issue-explorer",
  "displayName": "VS Code Issue Explorer",
  "description": "A VS Code extension that helps to create and explore GitHub issues within your IDE.",
  "version": "1.0.0-edge.5",
  "license": "MIT",
  "publisher": "stateful",
  "icon": "assets/icon.png",
  "galleryBanner": {
    "color": "#2F2F2F",
    "theme": "dark"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stateful/vscode-issue-explorer.git"
  },
  "engines": {
    "vscode": "^1.68.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "vscode",
    "extension",
    "best practices"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/index.js",
  "extensionKind": [
    "ui"
  ],
  "contributes": {
    "configuration": [],
    "commands": [
      {
        "command": "issue-explorer.createIssueFromSelection",
        "title": "Create Issue From Selection",
        "category": "Issue Explorer",
        "enablement": "editorHasSelection"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "issue-explorer",
          "title": "Issue Explorer",
          "icon": "assets/icon.png"
        }
      ]
    },
    "views": {
      "issue-explorer": [
        {
          "id": "create-issue",
          "type": "webview",
          "visibility": "visible",
          "name": "Create Issue"
        }
      ]
    },
    "menus": {
      "editor/context": [
        {
          "command": "issue-explorer.createIssueFromSelection",
          "group": "1_modification"
        }
      ]
    }
  },
  "scripts": {
    "build": "run-s clean compile",
    "clean": "rimraf out",
    "compile": "rollup -c ./rollup.config.mjs",
    "test": "run-s test:*",
    "test:lint": "eslint src --ext ts",
    "test:unit": "vitest",
    "test:e2e": "cd tests/e2e && npx wdio run ./wdio.conf.ts",
    "watch": "npm run clean && npm run compile -- --watch"
  },
  "devDependencies": {
    "@open-wc/testing": "^3.1.6",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "18.x",
    "@types/vscode": "^1.68.0",
    "@types/vscode-webview": "^1.57.0",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "@vscode/test-electron": "^2.1.5",
    "@wdio/cli": "^7.20.5",
    "@wdio/local-runner": "^7.20.5",
    "@wdio/mocha-framework": "^7.20.3",
    "@wdio/spec-reporter": "^7.20.3",
    "eslint": "^8.19.0",
    "eta": "^1.12.3",
    "jsdom": "^20.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.76.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-eta": "^1.0.0",
    "ts-node": "^10.8.2",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "vitest": "^0.19.1",
    "vsce": "^2.9.2",
    "wdio-vscode-service": "^3.0.3",
    "webdriverio": "^7.20.5"
  },
  "dependencies": {
    "@octokit/rest": "^19.0.3",
    "@vscode/codicons": "^0.0.31",
    "@vscode/extension-telemetry": "^0.6.2",
    "@vscode/webview-ui-toolkit": "^1.0.0",
    "lit": "^2.2.7",
    "lit-element": "^3.2.1",
    "shrink-path": "^0.3.1",
    "tangle": "^2.1.0"
  }
}
